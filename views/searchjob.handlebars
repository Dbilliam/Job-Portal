<div id="jobSearchPageId"></div>
<div id="jobList">
  <div class="container mb-3">
    <h1>
      <center style="color:#17a2b8">Assam Job Portal</center>
    </h1>
    <br>
    <br>
    <div class="row">
      <div class="col-md-2">
      </div>
      <div class="col-md-8">
        <div class="qsb-header-text">
