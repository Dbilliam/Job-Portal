            <option value="legal-secretary">Legal Secretary</option>
            <option value="personal-assistant">Personal Assistant</option>
            <option value="construction-builders">Construction Builder</option>
            <option value="labour">Labour Work</option>
            <option value="assistant">Assistant</option>
            <option value="shop-assistant">Shop Assistant</option>
            <option value="other">Other</option>
          </select>
          <button type="submit" value="Search" id="searchJob"
            class="MuiButtonBase-root MuiButton-root jss122 MuiButton-contained MuiButton-containedPrimary">Tab
            Search</button>
          <div id="loader" style="margin-top:10px; display:none;">
            <img src="/static/ajax-loader.gif">
          </div>
        </form>
        <div class="col-md-2">
        </div>
      </div>
      <br><br>
    </div>
    <div class="row mr-2 ml-2" id="jobs">
      <div class="col-md-2"> </div>
      <div class="col-md-8 col-lg-8 align-items-center">
        {{>govtjob}}
        {{#each job}}
        {{>jobsfinder}}
        {{/each}}
      </div>
      <div class="col-md-2"></div>
    </div>
  </div>
</div>
<script src="/js/jobsearch.js"></script>